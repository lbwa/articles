{
  "compilerOptions": {
    "outDir": "scripts/dist",
    "target": "es5",
    "lib": [
      "es2015",
      "dom"
    ],
    "allowJs": true,
    "sourceMap": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noImplicitAny": true
  },
  "include": [
    "scripts/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
